{
  "name": "transimg",
  "version": "1.0.0",
  "description": "一款将网页上的图片快速上传到图床，简单、快速、好用的Chrome扩展程序",
  "scripts": {
    "dev": "rimraf dist && cross-env NODE_ENV=development babel-node ./node_modules/.bin/webpack --watch --config build/webpack.config.js --progress --hide-modules",
    "prod": "rimraf dist && cross-env NODE_ENV=production babel-node ./node_modules/.bin/webpack --config build/webpack.config.js --progress --hide-modules && zip -q -r extension.zip dist"
  },
  "standard": {
    "parser": "babel-eslint",
    "globals": [
      "FormData",
      "Blob",
      "Image",
      "chrome"
    ],
    "ignore": [
      "**/node_modules/**"
    ]
  },
  "dependencies": {
    "vue": "^2.6.11"
  },
  "devDependencies": {
    "@babel/cli": "7.10.1",
    "@babel/core": "7.10.2",
    "@babel/node": "7.10.1",
    "@babel/plugin-proposal-object-rest-spread": "7.10.1",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/plugin-syntax-jsx": "^7.10.1",
    "@babel/plugin-transform-runtime": "7.10.1",
    "@babel/preset-env": "7.10.2",
    "axios": "^0.19.2",
    "babel-eslint": "^10.1.0",
    "babel-helper-vue-jsx-merge-props": "^2.0.3",
    "babel-loader": "^8.1.0",
    "babel-plugin-transform-vue-jsx": "4.0.1",
    "babel-preset-minify": "^0.5.1",
    "copy-webpack-plugin": "^6.0.2",
    "cross-env": "^7.0.2",
    "css-loader": "^3.5.3",
    "eslint-plugin-html": "^6.0.2",
    "file-loader": "^6.0.0",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "html-webpack-plugin": "^4.3.0",
    "less": "^3.11.3",
    "less-loader": "^6.1.0",
    "mini-css-extract-plugin": "^0.9.0",
    "raw-loader": "^4.0.1",
    "rimraf": "^3.0.2",
    "standard": "^14.3.4",
    "style-loader": "^1.2.1",
    "url-loader": "^4.1.0",
    "vue-loader": "^15.9.2",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0",
    "webpack-manifest-plugin": "^2.2.0"
  },
  "license": "MIT"
}
